{
  "name": "@fluencesh/fluence.lib.sc.sdk.generator",
  "version": "2.0.8",
  "private": false,
  "license": "UNLICENSED",
  "description": "Ethereum Blockchain Plugin",
  "author": "Andrew Zubko <andrew@applicature.com>",
  "engines": {
    "node": ">=4.8.0",
    "npm": ">=2.15.11",
    "yarn": ">=0.20.3"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "gulp build",
    "lint": "tslint '{index.ts,src/**/*.ts,test/**/*.ts}'",
    "precommit": "npm run lint ; npm run test:integration",
    "test": "jest",
    "test:ci": "jest --coverage --forceExit --runInBand",
    "test:lint": "tslint --force -t json --project ./tsconfig.json '{index.ts,src/**/*.ts,test/**/*.ts}'",
    "test:sonar": "sonar-scanner -Dsonar.host.url=https://sonarcloud.io -Dsonar.login=$SONARCLOUD_TOKEN"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.com:applicature/fluence.libs-monorepo.git"
  },
  "dependencies": {
    "archiver": "^3.0.0",
    "config": "^1.30.0",
    "lodash": "^4.17.11",
    "typescript": "^2.9.2",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@types/archiver": "^2.1.2",
    "@types/config": "0.0.33",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.116",
    "@types/node": "^8.10.11",
    "@types/winston": "^2.3.7",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-sourcemaps": "^2.6.3",
    "gulp-tslint": "^8.1.2",
    "gulp-typescript": "^3.2.3",
    "jest": "^21.2.1",
    "jest-junit": "^3.7.0",
    "jest-multi-test-result-processor": "^1.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "nsp": "^3.1.0",
    "sonarqube-scanner": "^2.1.0",
    "source-map-support": "^0.5.5",
    "ts-jest": "^21.2.4",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1"
  },
  "files": [
    "@types/",
    "dist/",
    "src/"
  ],
  "jest-multi-test-result-processor": {
    "processors": [
      "jest-junit",
      "jest-sonar-reporter"
    ]
  },
  "jestSonar": {
    "reportPath": "./test-reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "jest-junit": {
    "output": "./test-reports/junit.xml"
  }
}
