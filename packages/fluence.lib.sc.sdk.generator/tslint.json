{
    "extends": "tslint:recommended",
    "rules": {
        "array-type": [true, "generic"],
        "interface-name": false,
        "no-trailing-whitespace": [true, "ignore-blank-lines"],
        "one-line": false,
        "trailing-comma": false,
        "quotemark": [true, "single"],
        "no-namespace": false
    }
}