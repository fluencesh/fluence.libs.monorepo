{
    "name": "@applicature-private/fluence.lib.services",
    "version": "2.0.4",
    "private": false,
    "license": "UNLICENSED",
    "description": "Fluence Services Plugin",
    "author": "Andrew Zubko <andrew@applicature.com>",
    "engines": {
        "node": ">=4.8.0",
        "npm": ">=2.15.11",
        "yarn": ">=0.20.3"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "gulp build",
        "lint": "tslint '{index.ts,src/**/*.ts,test/**/*.ts}'",
        "precommit": "npm run lint ; npm run test:integration",
        "test": "jest",
        "test:ci": "jest --coverage --forceExit",
        "test:lint": "tslint --force -t json --project ./tsconfig.json '{index.ts,src/**/*.ts,test/**/*.ts}'",
        "test:sonar": "sonar-scanner -Dsonar.host.url=https://sonarcloud.io -Dsonar.login=$MULTIVEST_BACKEND_SONARCLOUD_TOKEN"
    },
    "repository": {
        "type": "git",
        "url": "git@bitbucket.com:applicature/fluence.libs-monorepo.git"
    },
    "dependencies": {
        "@applicature-private/core.dynamodb": "^2.0.1",
        "@applicature-private/core.metrics": "^2.0.0",
        "@applicature-private/core.mongodb": "^2.0.0",
        "@applicature-private/core.plugin-manager": "^2.0.0",
        "@aws/dynamodb-data-mapper-annotations": "^0.7.3",
        "agenda": "^1.0.3",
        "aws-sdk": "^2.317.0",
        "axios": "^0.18.0",
        "bignumber.js": "^7.2.1",
        "config": "^1.29.2",
        "ethereumjs-util": "^5.2.0",
        "faker": "^4.1.0",
        "jsonwebtoken": "^8.2.1",
        "lodash": "^4.17.11",
        "mongodb": "^2.2.28",
        "prom-client": "^11.1.3",
        "randomstring": "^1.1.5",
        "uuid": "^3.2.1",
        "winston": "^2.4.4"
    },
    "devDependencies": {
        "@types/agenda": "^1.0.1",
        "@types/bignumber.js": "^5.0.0",
        "@types/config": "0.0.34",
        "@types/ethereumjs-util": "^5.1.1",
        "@types/faker": "^4.1.4",
        "@types/jest": "^23.3.3",
        "@types/jsonwebtoken": "^7.2.7",
        "@types/lodash": "^4.14.116",
        "@types/mongodb": "^2.2.23",
        "@types/nock": "^9.1.3",
        "@types/node": "^10.11.4",
        "@types/randomstring": "^1.1.6",
        "@types/uuid": "^3.4.4",
        "@types/winston": "^2.4.4",
        "gulp": "^3.9.1",
        "gulp-clean": "^0.4.0",
        "gulp-sourcemaps": "^2.6.4",
        "gulp-tslint": "^8.1.3",
        "gulp-typescript": "^5.0.0-alpha.3",
        "jest": "^23.6.0",
        "jest-cli": "^20.0.4",
        "jest-junit": "^5.2.0",
        "jest-multi-test-result-processor": "^1.0.0",
        "jest-sonar-reporter": "^2.0.0",
        "sonarqube-scanner": "^2.1.1",
        "ts-jest": "^22.4.2",
        "ts-node": "^4.1.0",
        "tslint": "^5.9.1",
        "typescript": "^2.8.1"
    },
    "jest-multi-test-result-processor": {
        "processors": [
            "jest-junit",
            "jest-sonar-reporter"
        ]
    },
    "jestSonar": {
        "reportPath": "./test-reports",
        "reportFile": "test-report.xml",
        "indent": 4
    },
    "jest-junit": {
        "output": "./test-reports/junit.xml"
    }
}
