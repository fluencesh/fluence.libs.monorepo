{
  "name": "@applicature/multivest.blockchain.bitcoin",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "description": "Bitcoin Blockchain Plugin",
  "author": "Andrew Zubko <andrew@applicature.com>",
  "engines": {
    "node": ">=4.8.0",
    "npm": ">=2.15.11",
    "yarn": ">=0.20.3"
  },
  "scripts": {
    "lint": "esw *.js src config --color",
    "precommit": "npm run lint ; npm run test ; npm run test:security",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors ./tests/ --recursive",
    "test:watch": "npm test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors ./tests --recursive",
    "test:check-coverage": "npm run test:coverage && istanbul check-coverage",
    "report-coverage": "nyc --reporter=lcov --reporter=text-lcov npm run test",
    "test:security": "nsp check"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.com:applicature/multivest.blockchain.bitcoin.git"
  },
  "dependencies": {
    "@applicature/multivest.core": "git+ssh://git@bitbucket.org/applicature/multivest.plugin-manager.git",
    "@applicature/multivest.mongodb.ico": "git+ssh://git@bitbucket.org/applicature/multivest.mongodb.ico.git",
    "bignumber.js": "^4.1.0",
    "config": "^1.26.1",
    "bitcoin-core": "^1.2.0",
    "bitcoinjs-lib": "^3.1.1",
    "bluebird": "^3.5.0",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "chai": "^4.0.2",
    "chai-subset": "^1.5.0",
    "cross-env": "^5.0.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-watch": "^3.1.0",
    "husky": "^0.13.4",
    "istanbul": "^0.4.5",
    "lodash.clone": "^4.5.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.2",
    "should": "^10.0.0",
    "supertest": "^1.2.0",
    "supertest-as-promised": "^4.0.2",
    "nsp": "^3.1.0"
  }
}
